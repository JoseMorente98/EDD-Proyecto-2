                      <TableColumn fx:id="columnIndex" prefWidth="100.0" text="Index" />
                      <TableColumn fx:id="columnFecha" prefWidth="150.0" text="Fecha y Hora" />
                        <TableColumn fx:id="columnNonce" prefWidth="150.0" text="Nonce" />
                        <TableColumn fx:id="columnPreviousHash" prefWidth="150.0" text="Previous Hash" />
