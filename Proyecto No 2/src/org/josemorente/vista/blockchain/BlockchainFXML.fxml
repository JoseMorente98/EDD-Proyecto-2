<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.josemorente.vista.blockchain.BlockchainFXMLController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1024.0" pickOnBounds="true">
         <image>
            <Image url="@../../recursos/wallpaper1.jpg" />
